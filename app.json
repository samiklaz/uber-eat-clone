{
  "expo": {
    "name": "uber-eat-clone",
    "slug": "uber-eat-clone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.samiklaz.ubereatclone"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Uber eat clone built with React native, YELP API, Redux and Google Places API",
    "githubUrl": "https://github.com/samiklaz/"
  }
}
